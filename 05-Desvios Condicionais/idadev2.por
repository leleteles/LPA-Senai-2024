programa
{
	
	funcao inicio()
	{
		inteiro idade
		escreva("Digite a sua idade: \n")
		leia (idade)

		se (idade,18)
		{
			escreva ("Você é de menor")
			}
		senao (idade<12)
		{
			escreva ("Você ja pode ser preso")
		}

			
	}

	
}
